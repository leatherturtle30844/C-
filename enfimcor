//Funo para o funcionamento do carregar
void carregar(){
	 cout << "C";
    pausar(120);
    cout << "A";
    pausar(120);
    cout << "R";
    pausar(120);
    cout << "R";
    pausar(120);
    cout << "E";
    pausar(120);
    cout << "G";
    pausar(120);
    cout << "A";
    pausar(120);
    cout << "N";
    pausar(120);
    cout << "D";
    pausar(120);
    cout << "O";
    pausar(120);
system("cls");
    cout << "CARREGANDO";
    pausar(120);
    cout << ".";
    pausar(500);
    cout << ".";
    pausar(500);
    cout << ".";
    pausar(500); 
system("cls");
	cout << "CARREGANDO";
	cout << ".";
    pausar(500);
    cout << ".";
    pausar(500);
    cout << ".";
    pausar(500); 
    system("cls");
cout << "CARREGANDO";
	cout << ".";
    pausar(500);
    cout << ".";
    pausar(500);
    cout << ".";
    pausar(500); 
    system("cls");  
  	cout << "CARREGANDO";
	cout << ".";
    pausar(500);
    cout << ".";
    pausar(500);
    cout << ".";
    pausar(500); 
    system("cls"); 
}

int main() {
	int escolha;
	setlocale (LC_ALL,"portuguese");
carregar();



while (true) {
    cout << " Bem-vindo ao MENU do QUBRA-CABEA do LOBO/OVELHA/COUVE!\n";
    cout << "Escolha uma opcao e aps digite o nmero respectivo e aperte ENTER aps digitar:\n";
    cout << " 1. Jogar (Quando o JOGO iniciar preste ateno as regras)\n";
    cout << " 2. Mudar cor do sistema\n";
    cout << " 3. Sair\n";
    cin >> escolha;

    switch (escolha) {
        case 1:
        	system("cls");
        	carregar();
        	tutorial();
        	system("cls");
        	cout<<"  ";
        	cout<<"  ";
 cout << "CADA ETAPA REPRESENTA A ORDEM PARA SE COMPLETAR O QUEBRA CABEA.\n";
 cout<<"  ETAPAS VO DE 1 a 5 (cada etapa com uma opo).\n";
  cout <<" PARA FINALIZAR CADA ESCOLHA DIGITE O NMERO RESPECTIVO DA OPO, APS DIGITALO APERTE ENTER.\n";
   cout << " BOM JOGO.\n";
   pausar(100);
   

    int etapa, escolha;

    

    for (etapa = 1; etapa <= 5; etapa++) {
        cout << "\nEtapa " << etapa << ":\n";
        switch (etapa) {
            case 1:
	cout<<setw(50)<<"|              |\033[34m~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\033[0m|              |\n";
	cout<<setw(50)<<"|              |\033[34m~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\033[0m|              |\n";
	cout<<setw(50)<<"|              |\033[34m~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\033[0m|              |\n";
	cout<<setw(50)<<"|   &=LOBO     |\033[34m~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\033[0m|              |\n";
	cout<<setw(50)<<"|              |\033[34m~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\033[0m|              |\n";
	cout<<setw(50)<<"|  \033[33m\\_/\033[0m=BARCO   |\033[34m~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\033[0m|              |\n";
	cout<<setw(50)<<"|              |\033[34m~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\033[0m|              |\n";
	cout<<setw(50)<<"|              |\033[34m~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\033[0m|              |\n";
	cout<<setw(50)<<"|   @=OVELHA   |\033[34m~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\033[0m|              |\n";
	cout<<setw(50)<<"|              |\033[34m~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\033[0m|              |\n";
	cout<<setw(50)<<"|              |\033[34m~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\033[0m|              |\n";
	cout<<setw(50)<<"|              |\033[34m~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\033[0m|              |\n";
	cout<<setw(50)<<"|   \033[32m%=COUVE\033[0m    |\033[34m~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\033[0m|              |\n";
	cout<<setw(50)<<"|              |\033[34m~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\033[0m|              |\n";
	cout<<setw(50)<<"|              |\033[34m~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\033[0m|              |\n";
	cout<<setw(50)<<"|              |\033[34m~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\033[0m|              |\n";
	cout<<setw(50)<<"|              |\033[34m~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\033[0m|              |\n";
	pausar(200);
                cout << "1. Leva a ovelha e deixa o lobo com a couve\n";
                cout << "2. Leva o lobo e deixa a ovelha com a couve\n";
                cout << "3. Leva a couve e deixa a ovelha com o lobo\n";
                cout << "Escolha: ";
                cin >> escolha;
                if (escolha != 1) {
                    cout << "Resposta errada! Tente novamente.\n";
                    etapa--;
                }
                if (escolha==1){
                	system("cls");
    	            anima1();
					cout << "\nParabns! escolha correta.\n";
			}
                break;
            
            case 2:
                cout << "1. Ele volta para casa sem nenhum dos dois\n";
                cout << "2. Ele volta para buscar a ovelha\n";
                cout << "3. Ele volta para buscar o lobo levar ao outro lado\n";
                cout << "Escolha: ";
                cin >> escolha;
                if (escolha != 3) {
                    cout << "Resposta errada! Tente novamente.\n";
                    etapa--;
                }
                if (escolha==3){
                	
                	system("cls");
                	anima2();
					cout << "\nParabns! escolha correta.\n";
			}
                break;
            
            case 3:
                cout << "1. O campons deixa o lobo l (ele no gosta de couve).\n";
                cout << "2. O campons deixa o lobo junto a ovelha e volta sozinho.\n";
                cout << "3. O campons deixa o lobo e leva de volta a ovelha.\n";
                cout << "Escolha: ";
                cin >> escolha;
                if (escolha != 3) {
                    std::cout << "Resposta errada! Tente novamente.\n";
                    etapa--;
                }
                 if (escolha==3){
                 	system("cls");
                 	anima3();
					cout << "\nParabns! escolha correta.\n";
			}
                break;
           
            case 4:
                cout << "1. Ele pega e vai embora porque j cansou.\n";
                cout << "2. Ele deixa a ovelha e volta sozinho.\n";
                cout << "3. Ele deixa a ovelha e leva a couve.\n";
                cout << "Escolha: ";
                cin >> escolha;
                if (escolha != 3) {
                    cout << "Resposta errada! Tente novamente.\n";
                    etapa--;
                }
                if (escolha==3){
                	system("cls");

			}
                break;
            
            case 5:
            	cout << "\nParabns! escolha correta.\n";
            	anima4();
                cout << "1. Ele volta e leva a ovelha.\n";
                cout << "2. Ele leva a couve de volta.\n";
                cout << "3. Ele leva o lobo de volta.\n";
                cout << "Escolha: ";
                cin >> escolha;
                if (escolha != 1) {
                    cout << "Resposta errada! Tente novamente.\n";
                    etapa--;
     
                }
                if (escolha==1){
                	anima5();
					  cout << "\nParabns! Voc descobriu a estratgia correta.\n";
                      cout << "A estratgia era:\n";
                      cout << "1. Leva a ovelha para o outro lado e a deixa l.\n";
                      cout << "2. Volta sozinho para o lado inicial.\n";
                      cout << "3. Agora, leva o lobo para o outro lado e deixa o lobo l.\n";
                      cout << "4. Retorna com a ovelha para o lado inicial.\n";
                      cout << "5. Deixa a ovelha e leva a couve para o outro lado.\n";
                      cout << "6. Volta sozinho para o lado inicial e traz a ovelha de volta.\n";
					cout<<" ";
					pausar(70000);
					system("cls");
					carregar();
					
				}
			break;	

        }
    }
  
		
        case 2:
            while (true) {
            	system("cls");
                cout << "Escolha uma cor:\n";
                cout << "1. Azul\n";
                cout << "2. Verde\n";
                cout << "3. Vermelho\n";
                cout << "4. Amarelo\n";
                cin >> escolha;


switch (escolha) {
                    case 1:
                        changeColor(9);
                        system("cls");
                        break;
                    case 2:
                        changeColor(10);
                        system("cls");
                        break;
                    case 3:
                        changeColor(12);
                        system("cls");
                        break;
                    case 4:
                        changeColor(14); 
                        system("cls");
                        break;
                    default:
                        cout << "Opcao invalida. Tente novamente.\n";
                        break;
                }

                if (escolha >= 1 && escolha <= 4) {
                    break;
                }
            }
            break;

        case 3:
            cout << "Obrigado por jogar! At a proxima!\n";
            return 0;

        default:
            cout << "Opcao invalida. Tente novamente.\n";
            system("cls");
            break;
    }
}

return 0;
}


